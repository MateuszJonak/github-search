[ignore]
.*/node_modules/.*
.*/build/.*
.*/config-overrides.js*
.*/flow-typed/.*

[include]

[libs]
flow-typed
./interfaces/global.js

[lints]

[options]
all=true
module.name_mapper='^\(.*\)\.module\.css$' -> 'CSSModule'
module.name_mapper='^\(.*\)\.module\.sass$' -> 'CSSModule'
module.name_mapper='^\(.*\)\.module\.scss$' -> 'CSSModule'
module.system=haste

[strict]
